{
    "name": "sabirivan/gendiff",
    "description": "Generator of differences in config files",
    "keywords": [
      "hexlet",
      "php",
      "project",
      "level2",
      "gendiff"
    ],
    "type": "project",
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Иван Сабиров",
            "email": "sabirivan@yandex.com"
        }
    ],
    "scripts": {
      "phpcs": "phpcs"
    },
    "require": {
      "docopt/docopt": "^1.0",
      "funct/funct": "^1.5"
    },
    "autoload": {
      "files": [
        "src/runner.php",
        "src/gendiff.php",
        "src/makeDiffAst.php",
        "src/selectRender.php",
        "src/renderers/pretty.php"
      ]
    },
    "bin": [
      "bin/gendiff"
    ],
    "require-dev": {
        "squizlabs/php_codesniffer": "3.*"
    }
}
